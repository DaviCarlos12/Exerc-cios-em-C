#include <stdio.h>

int main(){
    float dolar, real, cotacao;

    printf("Insira aqui seu valor em dólar: ");
    scanf("%f", &dolar);

    printf("Insira aqui o valor da cotação: ");
    scanf("%f", &cotacao);

    real = dolar * cotacao;

    printf("Seu valor em reais é de: %f", real);
     
    return 0;
}

